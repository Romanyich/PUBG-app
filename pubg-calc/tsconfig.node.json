{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "outDir": "./dist/functions",
    "isolatedModules": false,
    "noEmit": true,
    "target": "ES2020"
  },
  "include": ["netlify/functions/**/*", "netlify/functions/**/*.json"]
}
